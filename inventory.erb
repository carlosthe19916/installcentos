[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v1.5.1
openshift_image_tag=v1.5.1
openshift_public_hostname=console.sistcoop.org
openshift_master_default_subdomain=apps.console.sistcoop.org
openshift_hosted_metrics_deploy=false

openshift_hosted_metrics_storage_access_modes=['ReadWriteOnce']
openshift_hosted_metrics_storage_nfs_directory=/exports
openshift_hosted_metrics_storage_nfs_options='*(rw,root_squash)'
openshift_hosted_metrics_storage_volume_name=metrics
openshift_hosted_metrics_storage_volume_size=10Gi

[masters]
console.sistcoop.org openshift_schedulable=true

[nodes]
console.sistcoop.org openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
